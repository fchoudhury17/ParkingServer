<html>
    <head>
        <title>{{title}}</title>
        <link rel="stylesheet" type="text/css" href="DiagramButtons.css">
        <link rel="stylesheet" type="text/css" href="style.css">
        <div id = "header"> Please choose a parking spot and press submit at the bottom. </div>
        <br>
    </head>
    <body id = "seniorlot">
      <form method="POST" action="">
      <div id = "spots">
      <div id="topSpots" width = "5000px">
        {{#each nums}}
            <input type="radio" name="parkingSpot" id="{{this}}" value="{{this}}">
            <label class="top" for="{{this}}">{{this}}</label>
        {{/each}}
      </div>

      <div id="bottomSpots1">
        {{#each nums98}}
            <input type="radio" name="parkingSpot" id="{{this}}" value="{{this}}">
            <label class="bottom" for="{{this}}">{{this}}</label>
        {{/each}}

        <div class = "arrows">&darr;</div>  <div class = "arrows">&uarr;</div>

        {{#each nums86}}
            <input type="radio" name="group1" id="{{this}}">
            <label class="bottom" for="{{this}}">{{this}}</label>
        {{/each}}

        <div class = "arrows">&darr;</div>  <div class = "arrows">&uarr;</div>

        {{#each nums61}}
            <input type="radio" name="group1" id="{{this}}">
            <label class="bottom" for="{{this}}">{{this}}</label>
        {{/each}}
      </div>

    </div>

      <input type="submit" id="SubmitButton">
    </form>
  </body>

    <div class="footer">
    Created by: Shreyas Gupta &amp; Fahim Choudhury
    </div>
</html>
